includes:
    - config/extension.neon

parameters:
    # parameters of this extension
    type_coverage:
        return_type: 98
        param_type: 98
        property_type: 98
        print_suggestions: false

    level: 8

    paths:
        - src
        - tests

    checkGenericClassInNonGenericObjectType: false

    excludePaths:
        - "*/Fixture/*"
        - "*/Source/*"
